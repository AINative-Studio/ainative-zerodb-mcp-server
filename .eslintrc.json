{
  "root": true,
  "env": {
    "node": true,
    "es2021": true,
    "commonjs": true,
    "jest": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "commonjs"
  },
  "rules": {
    "no-console": ["warn", { "allow": ["error"] }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
